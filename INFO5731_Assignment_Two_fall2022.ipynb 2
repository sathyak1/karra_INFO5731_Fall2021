    "from nltk.stem import PorterStemmer\n",
    "\n",
    "stop = stopwords.words('english')\n",
    "print(stop)\n",
    "#2\n",
    "st=word_tokenize(st)\n",
    "print(st)\n",
    "words = [i for i in st.split() if i.lower() not in stop]\n",
    "text = \" \".join(words)\n",
    "#3lower text\n",
    "text=text.lower()\n",
